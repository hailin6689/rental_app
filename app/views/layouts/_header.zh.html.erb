<script language="javascript">
    $(document).ready(function () {
        $(".location").click(function () {
            $("#cityPop").toggle();
        });
        $('#q').keydown(function (e) {
            var key = window.event ? e.keyCode : e.which;
            if (key.toString() == "13") {
                $("#search_button").click();
            }
        });
        $("#search_button").click(function () {
            var q = $('#q').val();
            q.replace('/', '');
            q.replace('\\', '');
            var q = q == '输入您要入住地址、商圈、关键字' ? '' : 'q/' + encodeURIComponent($('#q').val());
            //alert("q=" + q);
            location.href = "<%=yield(:base_url) %>/list/" + q;
            //		location.href = "http://www.souduanzu.com/list/price_day/1500,2000/" + q;
        });

        $("ul.header_top_nav li").hover(function () {
            $(this).addClass("hover");
        }, function () {
            $(this).removeClass("hover");
        }).click(function () {
            $(this).addClass("selected");
            $("ul.header_top_nav li").not($(this)).removeClass("selected");
        });

        $("#citys-tab > a").click(function () {
            $('#cityPop').show();
            $("#citys-tab > a").removeClass('current');
            $("#citys > div").css({ "display": "none" });
            $(this).addClass('current');
            var className = '.' + $(this).attr("Id");
            $(className).css({ "display": "block" });
        });
        
        plot_chat();
    });
</script>
