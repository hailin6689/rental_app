              <div>
                <span class="TColor_535353" title=""
                  <%=captions[i]%>">
                  <%=captions[i]%>(<span class="room_price_pink">
                    <%=prices[i]%>
                  </span>)
                </span>
              </div>

              <span class="TColor_999">
                <%=sources[i]%>
              </span>

              <div class="clear"></div>
            </div>

            <div class="clear"></div>
       
          </li>
        <%
        i +=1
        end
        %>
        
      </ul>
  </div>

  <div class="float_right main_floatRight">
    <div class="img_padding">
    <img src="images/mobile.png" width="664" height="70"></img>
    </div>
    <div class="img_padding">
      <img src="images/househunt.png" width="664" height="70"></img>
    </div>
    <div class="img_padding">
      <img src="images/logo_big.png" width="664" height="145"></img>
    </div>
    
    <!--info section -->
    <div class="tabbed_nav_bgcolor">
      <ul class="tabbed_nav">
        <li class="selected">
          <a href="#">
            <h1>热门房间</h1>
          </a>
        </li>
        <li>
          <a href="#">特价房间</a>
        </li>
        <li>
          <a href="#">特色去处</a>
        </li>
        <li class="last">
          <a href="#">多人租房</a>
        </li>
        <li  class="tabbed_nav_more">
          <a href="#">更多热门攻略>></a>
        </li>
      </ul>
      
      <div class="clear"></div>
<%
